{
  "manifest_version": 2,
  "version": "0.0.1",
  "short_name": "React App",
  "name": "Store Flow Tracking",
  "description": "Sample application showing how to create an extension with Create React App",
  "browser_action": {
    "default_title": "Smart Flow Tracking",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["socket.io.js", "background.bundle.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.bundle.js"]
    }
  ],
  "icons": {
    "16": "rocket.png",
    "48": "rocket.png",
    "128": "rocket.png"
  },
  "options_page": "options.html",
  "permissions": ["storage", "activeTab", "identity", "identity.email", "notifications"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "devtools_page": "popup.html",
  "web_accessible_resources": ["popup.html"]
}
